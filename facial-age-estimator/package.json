{
  "name": "@codait/max-facial-age-estimator",
  "version": "0.1.0",
  "description": "Estimate the age of a face.",
  "main": "dist/max.ageest.cjs.js",
  "module": "dist/max.ageest.es.js",
  "jsdelivr": "dist/max.ageest.min.js",
  "unpkg": "dist/max.ageest.min.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "rollup": "rollup --config rollup.config.js",
    "uglify": "uglifyjs dist/max.ageest.js -mc --source-map --output dist/max.ageest.min.js",
    "copy": "ncp model dist/model",
    "build": "npm run clean && npm run rollup && npm run uglify",
    "test": "npm run build && jasmine test/test.js"
  },
  "author": "Daniel Jalova (https://github.com/djalova)",
  "license": "Apache-2.0",
  "keywords": [
    "age estimator",
    "model asset exchange",
    "max",
    "tensorflow.js",
    "tensorflowjs",
    "tensorflow",
    "tf.js",
    "tfjs",
    "machine learning"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CODAIT/max-tfjs-models.git"
  },
  "bugs": {
    "url": "https://github.com/CODAIT/max-tfjs-models/issues"
  },
  "homepage": "https://github.com/CODAIT/max-tfjs-models/tree/master/facial-age-estimator#readme",
  "dependencies": {
    "@tensorflow/tfjs-node": "1.x"
  },
  "devDependencies": {
    "canvas": "^2.5.0",
    "jasmine": "^3.4.0",
    "jimp": "^0.6.4",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.6",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "uglify-es": "^3.3.9"
  }
}
